{
  "name": "glider",
  "repository": "Glider",
  "version": "0.1.0",
  "title": "Glider",
  "description": "Stay afloat and try not to hit the ground in this infinite gliding game!",
  "keywords": [
    "Princeton",
    "COS426",
    "Glider"
  ],
  "scripts": {
    "start": "webpack-dev-server --mode development --output-public-path \"\"",
    "prebuild": "rimraf ./build && mkdirp ./build",
    "build": "npm run prebuild && webpack --mode production --progress --output-public-path \"\"",
    "format": "prettier --write '*.js' 'src/**/*.js'",
    "deploy": "npm run prebuild && webpack --mode production --progress && gh-pages -d build"
  },
  "dependencies": {
    "dat.gui": "^0.7.7",
    "simplex-noise": "^3.0.1",
    "three": "^0.116.0",
    "three-obj-loader": "^1.1.3",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.2.4",
    "file-loader": "^6.0.0",
    "gh-pages": "^2.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.3.0",
    "mkdirp": "^1.0.4",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-dev-server": "^3.10.3"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
